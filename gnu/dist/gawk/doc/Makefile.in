# Link gawk.1 to pgawk.1
install-data-hook:
	(cd $(DESTDIR)$(man1dir); \
	$(LN) gawk.1 pgawk.1 2>/dev/null ; \
	exit 0)

# Undo the above when uninstalling
uninstall-hook:
	cd $(DESTDIR)$(man1dir); rm -f pgawk.1 ; exit 0

