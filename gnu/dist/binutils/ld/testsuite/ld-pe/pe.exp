# This test can only be run on i386 PE/COFF platforms.
if { ![istarget i*86-*-cygwin*] && ![istarget i*86-*-pe]
     && ![istarget i*86-*-mingw*] } {
    return
}

set pe_tests {
    {".secrel32" "" "" {secrel1.s secrel2.s}
     {{objdump -s secrel.d}} "secrel.x"}
}

run_ld_link_tests $pe_tests
