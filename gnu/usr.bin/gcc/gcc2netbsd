%% srcs cc1

.include <../Makefile.cc1>
%% file cc1plus/Makefile
#	$NetBSD: gcc2netbsd,v 1.3 1996/01/04 05:34:58 phil Exp $

PROG=	cc1plus

%% srcs cc1plus

.include <../Makefile.cc1>
%% file cc1obj/Makefile
