PROG=p
SRCS=parse.c
WARNS=5
MKMAN=no
CPPFLAGS+=-g -O0
LDFLAGS+=-g -O0

.include <bsd.prog.mk>

t: ${PROG}
	./${PROG} gpgsigned-a.gpg
