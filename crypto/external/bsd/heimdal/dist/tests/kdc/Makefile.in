codesign-all:
	@if [ X"$$CODE_SIGN_IDENTITY" != X ] ; then \
		foo='$(bin_PROGRAMS) $(sbin_PROGRAMS) $(libexec_PROGRAMS)' ; \
		for file in $$foo ; do \
			echo "CODESIGN $$file" ; \
			codesign -f -s "$$CODE_SIGN_IDENTITY" $$file || exit 1 ; \
		done ; \
	fi

