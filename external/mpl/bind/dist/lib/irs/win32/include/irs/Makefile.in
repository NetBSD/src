srcdir =	@srcdir@
VPATH =		@srcdir@
top_srcdir =	@top_srcdir@

HEADERS =
SUBDIRS =
TARGETS =

@BIND9_MAKE_RULES@

installdirs:
	$(SHELL) ${top_srcdir}/mkinstalldirs ${DESTDIR}${includedir}/irs

install:: installdirs
	for i in ${HEADERS}; do \
		${INSTALL_DATA} ${srcdir}/$$i ${DESTDIR}${includedir}/irs || exit 1; \
	done
