$ORIGIN .
$TTL 300	; 5 minutes
example			IN SOA	ns3.example. hostmaster.example. (
				2010080900 ; serial
				20         ; refresh (20 seconds)
				20         ; retry (20 seconds)
				1814400    ; expire (3 weeks)
				3600       ; minimum (1 hour)
				)
example.		NS	ns4.example. ; fake
example.		A	10.53.0.4    ; fake
example.		AAAA	::1	     ; fake

ns4.example.		A	10.53.0.4
