// NS5

options {
	port @PORT@;
	listen-on port @PORT@ { 10.53.0.5; };
	pid-file "named.pid";
	nxdomain-redirect signed;
};

zone "." {
        type master;
        file "root.db.signed";
};

// An unsigned zone that ns6 has a delegation for.
zone "unsigned." {
        type master;
        file "unsigned.db";
};
