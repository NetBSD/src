$TTL 300
. 			IN SOA	marka.isc.org. a.root.servers.nil. (
				2010   	; serial
				600         	; refresh
				600         	; retry
				1200    	; expire
				600       	; minimum
				)
.			NS	a.root-servers.nil.
a.root-servers.nil.	A	10.53.0.6
moves.			NS	ns.server.
server.			NS	ns7.server.
