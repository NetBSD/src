options {
	query-source address 10.53.0.2 dscp 1;
	notify-source 10.53.0.2 dscp 2;
	transfer-source 10.53.0.2 dscp 3;
	port @PORT@;
	pid-file "named.pid";
	listen-on { 10.53.0.2; };
	listen-on-v6 { none; };
	recursion no;
	send-cookie yes;
	nocookie-udp-size 512;
};

zone "." {
	type master;
	file "root.db";
};
