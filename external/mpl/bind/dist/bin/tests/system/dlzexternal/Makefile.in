srcdir =	@srcdir@
VPATH =		@srcdir@
top_srcdir =	@top_srcdir@

VERSION=@BIND9_VERSION@

@BIND9_MAKE_INCLUDES@

CINCLUDES =	${DNS_INCLUDES} ${ISC_INCLUDES} @DST_OPENSSL_INC@
CDEFINES =	@CRYPTO@
CWARNINGS =

LIBS =		@LIBS@

SO_TARGETS =    driver.@SO@
TARGETS =	@SO_TARGETS@

SRCS =		driver.c

SO_OBJS         = driver.@O@
SO_SRCS         = driver.c

OBJS =

@BIND9_MAKE_RULES@

CFLAGS =	@CFLAGS@ @SO_CFLAGS@
SO_LDFLAGS =	@LDFLAGS@ @SO_LDFLAGS@

driver.@SO@: ${SO_OBJS}
	${LIBTOOL_MODE_LINK} @SO_LD@ ${SO_LDFLAGS} -o $@ driver.@O@

clean distclean::
	rm -f ${TARGETS}

distclean::
	rm -f ns1/named.conf
