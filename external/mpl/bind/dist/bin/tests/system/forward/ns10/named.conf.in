options {
	query-source address 10.53.0.10;
	notify-source 10.53.0.10;
	transfer-source 10.53.0.10;
	port @PORT@;
	pid-file "named.pid";
	listen-on { 10.53.0.10; };
	listen-on-v6 { none; };
	minimal-responses no;
};

zone "net." {
	type master;
	file "fakenet.zone";
};

zone "spoofed.net." {
	type master;
	file "spoofednet.zone";
};

zone "sub.local.net." {
	type master;
	file "fakesublocalnet.zone";
};

zone "net2" {
	type master;
	file "fakenet2.zone";
};

zone "net.example.lll" {
	type master;
	file "net.example.lll";
};

zone "sub.local.tld." {
	type master;
	file "fakesublocaltld.zone";
};
