$TTL	600

@               IN      SOA     dns1.changing. postmaster.changing. (
                        2               ;; serial
                        3600            ;; refresh period
                        1800            ;; retry interval
                        604800          ;; expire time
                        600 )           ;; default TTL

		IN	NS		dns1.changing.
			NS		dns2.changing.

dns1		IN	A		10.53.0.1
dns2		IN	A		10.53.0.2

a		IN	A		10.0.0.2
