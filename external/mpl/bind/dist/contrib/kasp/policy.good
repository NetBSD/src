# A default policy that will
# amaze you and your friends
policy Policy1 {
	algorithm RSASHA1;
	keyttl 60;
	key-size ksk 2048;
	key-size zsk 2048;
	roll-period ksk 2400;
	roll-period zsk 1500;
	standby ksk 1;
	standby zsk 1;
};

