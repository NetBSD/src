CC = @CC@
LIBOBJS = @LIBOBJS@
LIBS = @LIBS@
DEFS = @DEFS@

perftcpdns: perftcpdns.o $(LIBOBJS)
	$(CC) $(CFLAGS) $(DEFS) $(LDFLAGS) perftcpdns.o $(LIBOBJS) $(LIBS) -o perftcpdns

perftcpdns.o: perftcpdns.c
	$(CC) $(CFLAGS) $(DEFS) -c perftcpdns.c

clean:
	rm -f *.o perftcpdns

distclean: clean
	rm -f config.log
	rm -f config.cache
	rm -f config.status
	rm -f Makefile
