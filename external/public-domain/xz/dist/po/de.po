#, c-format
msgid "  Streams:            %s\n"
msgstr "  Ströme:              %s\n"

#: src/xz/list.c:654
#, c-format
msgid "  Blocks:             %s\n"
msgstr "  Blöcke:              %s\n"

#: src/xz/list.c:656
#, c-format
