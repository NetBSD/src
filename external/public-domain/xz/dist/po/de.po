msgstr "  Größe komprimiert:   %s\n"

#: src/xz/list.c:659
#, c-format
msgid "  Uncompressed size:  %s\n"
msgstr "  Größe unkomprimiert: %s\n"

#: src/xz/list.c:662
#, c-format
msgid "  Ratio:              %s\n"
msgstr "  Verhältnis:          %s\n"

#: src/xz/list.c:664
#, c-format
msgid "  Check:              %s\n"
msgstr "  Check:               %s\n"

#: src/xz/list.c:665
#, c-format
msgid "  Stream padding:     %s\n"
