#, c-format
msgid "  Streams:            %s\n"
msgstr "  Flux :                            %s\n"

#: src/xz/list.c:654
#, c-format
msgid "  Blocks:             %s\n"
msgstr "  Blocs :                           %s\n"

#: src/xz/list.c:656
#, c-format
msgid "  Compressed size:    %s\n"
msgstr "  Taille compressé :                %s\n"

#: src/xz/list.c:659
#, c-format
msgid "  Uncompressed size:  %s\n"
msgstr "  Taille décompressé :              %s\n"

#: src/xz/list.c:662
#, c-format
msgid "  Ratio:              %s\n"
msgstr "  Ratio :                           %s\n"

#: src/xz/list.c:664
#, c-format
msgid "  Check:              %s\n"
msgstr "  Vérification :                    %s\n"

#: src/xz/list.c:665
#, c-format
msgid "  Stream padding:     %s\n"
msgstr "  Octets de rembourrage du flux :   %s\n"

