$as_echo "$gl_cv_func_re_compile_pattern_broken" >&6; }
    ac_use_included_regex=$gl_cv_func_re_compile_pattern_broken
    ;;
  *) as_fn_error "Invalid value for --with-included-regex: $with_included_regex" "$LINENO" 5
