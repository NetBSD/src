#: src/diff.c:498
#, c-format
msgid "invalid width `%s'"
msgstr "nevalida larøo '%s'"

#: src/diff.c:502
msgid "conflicting width options"
msgstr "malkongruaj opcioj pri larøo"

#: src/diff.c:526
#, c-format
