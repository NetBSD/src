#
# Makefile.in for top-level of pcc.
#

@SET_MAKE@
VPATH=@srcdir@
CC=@CC@
CFLAGS=@CFLAGS@ @ADD_CFLAGS@
LDFLAGS=@LDFLAGS@
CPPFLAGS=@CPPFLAGS@
YACC=@YACC@
LEX=@LEX@

SUBDIR=cc

all: ${SUBDIR}

install:
	cd cc && ${MAKE} install

clean:
	cd cc && ${MAKE} clean

distclean:
	cd cc && ${MAKE} distclean
	cd f77 && ${MAKE} distclean
	/bin/rm -rf Makefile config.log stamp-h1 config.status \
	configure.lineno config.h autom4te.cache

${SUBDIR}: nonexistant
	cd $@ && $(MAKE) all $(MFLAGS)

nonexistant:
