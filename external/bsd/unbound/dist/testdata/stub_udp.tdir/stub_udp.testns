; nameserver test file
$ORIGIN example.com.
$TTL 3600

ENTRY_BEGIN
MATCH opcode qtype qname
REPLY QR AA NOERROR
ADJUST copy_id
SECTION QUESTION
www	IN	A
SECTION ANSWER
www	IN	A	10.20.30.40
SECTION AUTHORITY
@	IN 	NS	ns.example.com.
SECTION ADDITIONAL
ns	IN	A	127.0.0.1
ENTRY_END

ENTRY_BEGIN
MATCH opcode qtype qname
REPLY QR AA NOERROR
ADJUST copy_id
SECTION QUESTION
mail	IN	A
SECTION ANSWER
mail	IN	A	10.20.30.66
SECTION AUTHORITY
@	IN 	NS	ns.example.com.
SECTION ADDITIONAL
ns	IN	A	127.0.0.1
ENTRY_END

; root prime
ENTRY_BEGIN
MATCH opcode qtype qname
REPLY QR AA NOERROR
ADJUST copy_id
SECTION QUESTION
.	IN	NS
SECTION ANSWER
. 	IN 	NS 	root.server.
SECTION AUTHORITY
SECTION ADDITIONAL
root.server.	IN	A	127.0.0.1
ENTRY_END

