srcdir =	@srcdir@
VPATH =		@srcdir@
top_srcdir =	@top_srcdir@

@BIND9_MAKE_INCLUDES@

SUBDIRS =	builtin dlzexternal filter-aaaa geoip lwresd rpz rsabigexponent tkey tsiggss
TARGETS =

@BIND9_MAKE_RULES@

# Running the scripts below is bypassed when a separate
# build directory is used.

check: test

test: subdirs
	if test -f ./runall.sh; then sh ./runall.sh; fi

testclean clean distclean::
	if test -f ./cleanall.sh; then sh ./cleanall.sh; fi
	rm -f systests.output
	rm -f random.data

distclean::
	rm -f conf.sh

installdirs:

install::
