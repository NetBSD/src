# Echo the interface flags, reason and message options

if [ "$reason" = "TEST" ]; then
	set | grep \
	    "^\(interface\|pid\|reason\|protocol\|profile\|skip_hooks\)=" | sort
	set | grep "^if\(carrier\|flags\|mtu\|wireless\|ssid\)=" | sort
	set | grep "^\(new_\|old_\|nd[0-9]*_\)" | sort
	exit 0
fi
