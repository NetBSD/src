# Just echo our DHCP options we have

if [ "$reason" = "TEST" ]; then
	set | grep "^\(interface\|pid\|reason\|skip_hooks\)=" | sort
	set | grep "^if\(carrier\|flags\|mtu\|wireless\|ssid\)=" | sort
	set | grep "^\(new_\|old_\|ra_count=\|ra[0-9]*_\)" | sort
	exit 0
fi
