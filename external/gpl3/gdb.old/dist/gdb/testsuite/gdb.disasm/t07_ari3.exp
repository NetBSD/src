set srcfile ${srcdir}/${subdir}/${testfile}.s
set objfile ${objdir}/${subdir}/${testfile}.o
set binfile ${objdir}/${subdir}/${testfile}.x

set asm-flags ""
set link-flags "-m h8300sxelf"


if {[target_assemble $srcfile $objfile "${asm-flags}"] != ""} then {
