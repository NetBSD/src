standard_testfile .c psym-external-decl-2.c

if { [test_compiler_info "clang-*"] } {
    return -1
}

set srcfiles [list $srcfile $srcfile2]

if { [build_executable_from_specs \
	  "failed to prepare" \
	  $testfile [list] \
	  $srcfile [list debug] \
	  $srcfile2 [list]] == -1 } {
    return -1
}

clean_restart $testfile

gdb_test "print aaa" " = 33"
