standard_testfile

if {[prepare_for_testing "failed to prepare" $testfile $testfile.c]} {
    return -1
}

if {![runto_main]} {
    return -1
}

# Make sure we do not see any warnings.
gdb_test_multiple "next" "next over exit" {
    -re "$inferior_exited_re normally.\[\r\n\]+$gdb_prompt $" {
	pass "next over exit"
    }
}
