# Test pretty printing of structures and unions.

standard_testfile

if {[prepare_for_testing $testfile.exp $testfile $srcfile debug]} {
    untested $testfile.exp
    return -1
}

if ![runto_main] {
    return -1
}

gdb_test_no_output "set print pretty on"

gdb_test "print s1" \
    [multi_line \
	 " = {" \
	 "  one = 1," \
	 "  two = 2," \
	 "  data = {" \
	 "    {" \
	 "      three = 7," \
	 "      four = -1" \
	 "    }," \
	 "    {" \
	 "      five = -1," \
	 "      six = 15" \
	 "    }" \
	 "  }" \
	 "}" ]

gdb_test "print s2" \
    [multi_line \
	 " = {" \
	 "  one = 1," \
	 "  two = 2," \
	 "  {" \
	 "    three = 3," \
	 "    four = 4" \
	 "  }" \
	 "}" ]
