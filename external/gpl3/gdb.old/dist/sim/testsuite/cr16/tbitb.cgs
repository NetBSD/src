# cr16 testcase for tbitb
# mach:	 cr16

	.include "testutils.inc"

	start

	.global tbitb
tbitb:
	movw $0, r1
	lpr     r1, psr
	movw $_y, r1
	tbitb	$0, 0(r1)
	spr  psr, r1
	cmpb    $0x20, r1
	beq ok1
not_ok:
	fail

ok1:
	movw $0, r1
	lpr     r1, psr
	movd   $_y, (r1,r0)
	tbitb	$1,0(r1,r0)
	spr  psr, r1
	cmpb    $0x20, r1
	beq ok2
	br not_ok
ok2:

	pass

_y:	.word	0xf7
