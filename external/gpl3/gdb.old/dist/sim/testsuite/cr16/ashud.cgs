# cr16 testcase for ashud $sr,$dr
# mach(): cr16

	.include "testutils.inc"

	start

	.global ashud
ashud:

	movd  $0xf1234567, (r4,r3)
	movw  $20,r5
	ashud r5, (r4,r3)

	cmpd $0x56700000, (r4,r3)
	beq ok
not_ok:
	fail
ok:
	movd  $0xf1234567, (r4,r3)
	movw  $-20,r5
	ashud r5, (r4,r3)

	test_h_grp "(r4,r3)", -238

	pass
