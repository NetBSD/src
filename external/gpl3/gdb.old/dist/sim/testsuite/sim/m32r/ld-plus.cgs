# m32r testcase for ld $dr,@$sr+
# mach(): m32r m32rx

	.include "testutils.inc"

	start

	.global ld_plus
ld_plus:
	mvaddr_h_gr r4, data_loc
	mvi_h_gr    r5, 0

	ld r5, @r4+

	test_h_gr r5, 0x12345678

	mvaddr_h_gr r5, data_loc2
	bne r4, r5, not_ok

	pass
not_ok:
	fail

data_loc:
	.word 0x12345678
data_loc2:
	.word 0x11223344

