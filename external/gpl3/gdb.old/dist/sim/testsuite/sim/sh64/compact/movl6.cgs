# sh testcase for mov.l @$rm, $rn -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start

	mov #30, r0
	shll8 r0
	# Store something there first.
	mov #170, r1
	mov.l r1, @r0
check:	
	# Load it back.
	mov.l @r0, r3
	cmp/eq r1, r3
	bf wrong

okay:
	pass
wrong:
	fail
	
