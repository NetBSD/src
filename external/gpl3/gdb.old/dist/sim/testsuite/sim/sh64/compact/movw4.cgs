# sh testcase for mov.w r0, @($imm8x2, gbr) -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start

	mov #30, r0
	shll8 r0
	ldc r0, gbr

init:
	# Build up a distinctive bit pattern.
	mov #1, r0
	shll8 r0
	add #12, r0
	# Preserve r0.
	mov r0, r7
	mov.w r0, @(12, gbr)
check:
	mov.w @(12, gbr), r0
	cmp/eq r0, r7
	bf wrong

okay:	
	pass
wrong:
	fail
	
