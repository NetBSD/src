# sh testcase for fmul.s $frg, $frh, $frf -*- Asm -*-
# mach: all
# as: -isa=shmedia
# ld: -m shelf64

	.include "media/testutils.inc"

	start

init:
	pta wrong, tr0

fmuls1:
	movi 2, r0
	fmov.ls r0, fr0
	float.ls fr0, fr0
	movi 3, r1
	fmov.ls r1, fr1
	float.ls fr1, fr1
	fmul.s fr0, fr1, fr2
	movi 6, r2
	fmov.ls r2, fr3
	float.ls fr3, fr3
	fcmpeq.s fr2, fr3, r7
	bnei r7, 1, tr0

okay:
	pass

wrong:
	fail
