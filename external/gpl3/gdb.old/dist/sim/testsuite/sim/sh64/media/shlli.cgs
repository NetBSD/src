# sh testcase for shlli $rm, $imm6, $rd -*- Asm -*-
# mach: all
# as: -isa=shmedia
# ld: -m shelf64

	.include "media/testutils.inc"

	start

init:
	pta wrong, tr0

shlli:
	movi 1, r0
	shlli r0, 3, r0
	bnei r0, 8, tr0

shlli2:
	shlli r0, 3, r0

shlli3:
	# Shift all bits out of sight.
	shlli r0, 63, r0
	bnei r0, 0, tr0

okay:
	pass

wrong:
	fail
