# sh testcase for bge$likely $rm, $rn, $tra -*- Asm -*-
# mach: all
# as: -isa=shmedia
# ld: -m shelf64

	.include "media/testutils.inc"

	start

	.global bge
init:
	pta bge2, tr0
	pta bge3, tr1
	pta wrong, tr2
	movi 0, r0
	
bge1:
	# Compare r0 with itself.
	bge/l r0, r0, tr0
	# We should branch here.
	fail

bge2:
	movi 1, r1
	movi 1, r2
	bge r1, r2, tr1
	# We should branch here.
	fail

bge3:
	movi -1, r1
	movi 1, r2
	bge r1, r2, tr2
	# We should not branch here.

okay:
	pass

wrong:
	fail
