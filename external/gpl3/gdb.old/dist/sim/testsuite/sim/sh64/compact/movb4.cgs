# sh testcase for mov.b r0, @($imm8, gbr) -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start
	mov #0, r0
	or #170, r0
	mov r0, r3
	mov #30, r2
	ldc r2, gbr
	mov.b r0, @(40, gbr)

	# Load the value back into a different register.
	mov.b @(40, gbr), r0
	# Check the lowest order byte matches the stored value.
	and #255, r0
	cmp/eq r0, r3
	bf wrong

okay:	
	pass

wrong:
	fail
	
