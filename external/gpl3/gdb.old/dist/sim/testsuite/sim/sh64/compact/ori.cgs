# sh testcase for or #$imm8, r0 -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start

	.global ori
ori:
	mov #1, r0
	rotr r0
	or #1, r0

	mov #1, r7
	rotr r7
	add #1, r7
	cmp/eq r0, r7
	bf wrong

	.global ori2
ori2:
	mov #85, r0
	shll16 r0
	shll8 r0
	or #85, r0

	mov #85, r7
	shll16 r7
	shll8 r7
	add #85, r7
	cmp/eq r0, r7
	bf wrong

okay:
	pass
	
wrong:
	fail
