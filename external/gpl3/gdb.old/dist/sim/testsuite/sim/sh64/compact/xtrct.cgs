# sh testcase for xtrct $rm, $rn -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start

init:
	mov #170, r0
	shll8 r0
	add #1, r0
	shll8 r0
	add #66, r0
	shll8 r0
	mov r0, r1

	mov #85, r0
	shll8 r0
	add #2, r0
	shll8 r0
	add #42, r0
	shll8 r0
	add #3, r0

copy:
	mov r0, r3
	mov r1, r4

xtrct:
	xtrct r0, r1

check:
	# Lower r3, upper r4.
	shll16 r3
	shlr16 r4
	or r3, r4
	cmp/eq r1, r4
	bf wrong
	
okay:
	pass
wrong:
	fail
	
