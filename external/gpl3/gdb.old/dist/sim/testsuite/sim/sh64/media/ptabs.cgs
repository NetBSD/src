# sh testcase for ptabs$likely $rn, $tra -*- Asm -*-
# mach: all
# as: -isa=shmedia
# ld: -m shelf64

	.include "media/testutils.inc"

	start

	.global ptabs
ptabs:
	movi 16, r0
	shlli r0, 8, r0
	# Add one to stay in SHmedia mode.
	addi r0, 29, r0
	ptabs r0, tr0

	# Now jump.
	beqi r63, 0, tr0

wrong:
	fail

okay:
	pass
