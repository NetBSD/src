# sh testcase for fneg.s $frgh, $frf -*- Asm -*-
# mach: all
# as: -isa=shmedia
# ld: -m shelf64

	.include "media/testutils.inc"

	start
init:
	pta wrong, tr0
	movi 0, r0
	movi 1, r1

fnegs0:
	# Ensure fnegs(0) = 0.
	fmov.ls r0, fr7
	float.ls fr7, fr0
	fneg.s fr0, fr1
	fcmpeq.s fr0, fr1, r7
	bnei r7, 1, tr0

fnegs1:
	# Ensure fnegs(fnegs(1)) = 1.
	fmov.ls r1, fr7
	float.ls fr7, fr0
	fneg.s fr0, fr1
	fneg.s fr1, fr2
	fcmpeq.s fr0, fr2, r7
	bnei r7, 1, tr0

okay:
	pass

wrong:
	fail
