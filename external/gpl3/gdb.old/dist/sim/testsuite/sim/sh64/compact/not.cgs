# sh testcase for not $rm64, $rn64 -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start

	.global not
not:
	mov #0, r0
	or #192, r0
	not r0, r1

	mov #0, r0
	or #255, r0
	shll8 r0
	or #255, r0
	shll8 r0
	or #255, r0
	shll8 r0
	or #63, r0

	cmp/eq r0, r1
	bf wrong

ones:
	mov #0, r1
	not r1, r2

	mov #0, r0
	or #255, r0
	shll8 r0
	or #255, r0
	shll8 r0
	or #255, r0
	shll8 r0
	or #255, r0
	cmp/eq r0, r2
	bf wrong

okay:
	pass

wrong:
	fail
