# sh testcase for dt $rn -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start

	.global dt
dt:
	mov #3, r0
	dt r0
	bt wrong
	assert r0, #2

	mov #1, r0
	dt r0
	bf wrong
	assert r0, #0

	mov #0, r0
	dt r0
	bt wrong
	mov #0, r7
	not r7, r7
	cmp/eq r7, r0
	bf wrong

	mov #1, r0
	neg r0, r0
	dt r0
	mov #1, r7
	not r7, r7
	cmp/eq r7, r0
	bf wrong

okay:
	pass

wrong:
	fail
