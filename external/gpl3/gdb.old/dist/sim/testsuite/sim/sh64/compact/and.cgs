# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start

	.global and
and:
	mov #1, r1
	mov #7, r2
	rotr r2
	rotr r2
	and r1, r2

	# R1 & R2 = 1.
	assert r2, #1

another:
	mov #192, r1
	mov #0, r2
	and r1, r2

	# R1 & R2 = 0.
	assert r2, #0

okay:
	pass

wrong:
