# sh testcase for shlrd $rm, $rn, $rd -*- Asm -*-
# mach: all
# as: -isa=shmedia
# ld: -m shelf64

	.include "media/testutils.inc"

	start

init:
	pta wrong, tr0

shlrd1:
	movi 128, r0
	movi 3, r1
	shlrd r0, r1, r2
	bnei r2, 16, tr0

shlrd2:
	movi -4, r0
	movi 2, r1
	shlrd r0, r1, r2
	addi r2, 1, r2
	beqi r2, 0, tr0

okay:
	pass

wrong:
	fail
