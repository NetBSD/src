# sh testcase for lds.l @${rn}+, pr -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start

	.global ldsl_pr
ldsl_pr:
	mov #40, r0
	shll8 r0
	# Preserve address.
	mov r0, r1
	lds.l @r0+, pr

	# Add 4 to saved address (r1).
	# Then compare with r0.
	add #4, r1
	cmp/eq r0, r1
	bf wrong

okay:
	pass

wrong:
	fail
