# sh testcase for mov.b @${rm}+, $rn -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start
	mov #30, r1
	shll8 r1
	# Store addr.
	mov r1, r8

	# Store something there first.
	mov #0, r0
	or #170, r0
	mov r0, r7
	mov.b r7, @r1
	# Load it back.
	mov.b @r1+, r2
	mov r2, r0
	and #255, r0
	cmp/eq r7, r0
	bf wrong

	# Test address for post-incrementing.
	add #1, r8
	cmp/eq r8, r1
	bf wrong

okay:	
	pass
wrong:
	fail
	
