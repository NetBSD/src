# sh testcase for mov.w r0, @($imm4x2, $rn) -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start
	mov #30, r1
	shll8 r1

init:
	# Build up a distinctive bit pattern.
	mov #1, r0
	shll8 r0
	add #12, r0
	# Preserve.
	mov r0, r7
move:
	mov.w r0, @(12, r1)
check:
	mov.w @(12, r1), r0
	shll16 r0
	shll16 r7
	cmp/eq r0, r7
	bf wrong

okay:
	pass
wrong:
	fail
	
