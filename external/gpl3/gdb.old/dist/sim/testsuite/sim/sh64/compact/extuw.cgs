# sh testcase for extu.w $rm, $rn -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start

	.global extuw
extuw:
	mov #42, r1
	extu.w r1, r2
	assert r2, #42
	
another:
	mov #0, r0
	or #255, r0
	shll8 r0
	extu.w r0, r1
	mov #0, r0
	or #255, r0
	shll8 r0
	cmp/eq r0, r1
	bf wrong

okay:
	pass

wrong:
	fail
