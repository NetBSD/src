# sh testcase for shlrd.l $rm, $rn, $rd -*- Asm -*-
# mach: all
# as: -isa=shmedia
# ld: -m shelf64

	.include "media/testutils.inc"

	start

init:
	pta wrong, tr0

shlrdl1:
	movi 1, r0
	shlli r0, 32, r0
	ori r0, 8, r0
	movi 1, r1
	shlli r1, 7, r1
	ori r1, 3, r1
	
	shlrd.l r0, r1, r2
	
check1:
	bnei r2, 1, tr0

shlrdl2:
	movi 1, r0
	shlli r0, 31, r0
	movi 31, r1
	shlld.l r0, r1, r2
	bnei r2, 0, tr0

okay:
	pass

wrong:
	fail
