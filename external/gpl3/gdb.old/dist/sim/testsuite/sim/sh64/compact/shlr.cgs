# sh testcase for shlr $rn -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start

	.global shlr
shlr:
	mov #0, r0
	or #192, r0
	shlr r0
	shlr r0
	shlr r0
	shlr r0
	shlr r0
	shlr r0
	# Make sure a bit is shifted into T.
	shlr r0
	bf wrong
	# Ditto.
	shlr r0
	bf wrong
	shlr r0
	assert r0, #0

okay:	
	pass

wrong:
	fail
