# sh testcase for mov.b @(r0, $rm), $rn -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start
	mov #30, r0
	shll8 r0
	mov #14, r1
	# Store something there first.
	mov #0, r0
	or #170, r0
	mov r0, r7
	mov.b r7, @(r0, r1)
	# Load it back.
	mov.b @(r0, r1), r2
	mov r2, r0
	and #255, r0
	cmp/eq r0, r7
	bf wrong

okay:
	pass
wrong:
	fail
