# sh testcase for fcmpgt.s $frg, $frh, $rd -*- Asm -*-
# mach: all
# as: -isa=shmedia
# ld: -m shelf64

	.include "media/testutils.inc"

	start

init:
	pta wrong, tr0

fcmpgt1:
	movi 2, r0
	fmov.ls r0, fr0
	movi 1, r1
	fmov.ls r1, fr1
	float.ls fr0, fr2
	float.ls fr1, fr3
	fcmpgt.s fr2, fr3, r7
	bnei r7, 1, tr0 

fcmpgt2:
	movi 1, r0
	fmov.ls r0, fr0
	fmov.ls r0, fr1
	float.ls fr0, fr2
	float.ls fr1, fr3
	fcmpgt.s fr2, fr3, r7
	bnei r7, 0, tr0

okay:
	pass

wrong:
	fail
