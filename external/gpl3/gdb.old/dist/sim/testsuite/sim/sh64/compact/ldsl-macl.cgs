# sh testcase for lds.l @${rn}+, macl -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start

	.global ldsl_macl
ldsl_macl:
	mov #40, r0
	shll8 r0
	# save address for later examination.
	mov r0, r1

	lds.l @r0+, macl

	add #4, r1
	cmp/eq r0, r1
	bf wrong

okay:
	pass
wrong:
	fail
