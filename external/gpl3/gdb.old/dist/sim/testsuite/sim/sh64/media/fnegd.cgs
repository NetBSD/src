# sh testcase for fneg.d $drgh, $drf -*- Asm -*-
# mach: all
# as: -isa=shmedia
# ld: -m shelf64

	.include "media/testutils.inc"

	start
init:
	pta wrong, tr0
	movi 0, r0
	movi 1, r1

fnegd0:
	# Ensure fnegd(0) = 0.
	fmov.ls r0, fr7
	float.ld fr7, dr0
	fneg.d dr0, dr2
	fcmpeq.d dr0, dr2, r7
	bnei r7, 1, tr0

fnegd1:
	# Ensure fnegd(fnegd(1)) = 1.
	fmov.ls r1, fr7
	float.ld fr7, dr0
	fneg.d dr0, dr2
	fneg.d dr2, dr4
	fcmpeq.d dr0, dr4, r7
	bnei r7, 1, tr0
	
okay:
	pass

wrong:
	fail
