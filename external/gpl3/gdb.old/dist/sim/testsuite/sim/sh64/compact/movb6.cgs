# sh testcase for mov.b @$rm, $rn -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start
	mov #30, r2
	shll8 r2
	# Store something first.
	mov #0, r0
	or #170, r0
	mov r0, r7
	mov.b r7, @r2
	# Load it back.
	mov.b @r2, r1
	mov r1, r0
	and #255, r0
	cmp/eq r7, r0
	bf wrong

okay:
	pass
wrong:
	fail
