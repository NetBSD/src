# sh testcase for stc.l gbr, @-$rn -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start

	.global stcl_gbr
stcl_gbr:
	mov #42, r0
	ldc r0, gbr
	mov #40, r0
	shll8 r0
	# save address
	mov r0, r1
	stc.l gbr, @-r0

	add #4, r0
	cmp/eq r0, r1
	bf wrong

okay:
	pass
wrong:
	fail
