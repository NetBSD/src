# sh testcase for mov.w @$rm, $rn -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start
	
	mov #30, r0
	shll8 r0

	# Store something first.
	# Build up a distinctive bit pattern.
	mov #1, r2
	shll8 r2
	add #12, r2
	mov.w r2, @r0

check:
	# Read it back.
	mov.w @r0, r1
	cmp/eq r1, r2
	bf wrong

okay:
	pass
wrong:
	fail
	
