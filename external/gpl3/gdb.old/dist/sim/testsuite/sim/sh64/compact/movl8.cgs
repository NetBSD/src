# sh testcase for mov.l @(r0, $rm), $rn -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start
	mov #0, r0
	mov #30, r1
	shll8 r1
	# Store something there first.
	mov #170, r3
	mov.l r3, @(r0, r1)
check:	
	# Load it back.
	mov.l @(r0, r1), r2
	cmp/eq r2, r3
	bf wrong

okay:	
	pass
wrong:	
	fail
