# sh testcase for mov.l $rm, @$rn -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start
	mov #30, r1
	shll8 r1
init:
	# Build up a distinctive bit pattern.
	mov #1, r2
	shll8 r2
	add #12, r2
	shll8 r2
	add #85, r2
	shll8 r2
	add #170, r2

	mov.l r2, @r1

	# Load it back.
	mov.l @r1, r3
	cmp/eq r2, r3
	bf wrong

okay:
	pass
wrong:
	fail
