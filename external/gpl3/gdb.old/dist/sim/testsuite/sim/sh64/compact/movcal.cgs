# sh testcase for movca.l r0, @$rn -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start

	.global movcal
movcal:
	mov #1, r0
	rotr r0
	add #128, r0
	mov #40, r1
	shll8 r1
	movca.l r0, @r1

	# Load the word back in.
	mov.l @r1, r3
	cmp/eq r0, r3
	bf wrong
	
okay:
	pass
wrong:
	fail
	
