# sh testcase for fsqrt.d $frgh, $frf -*- Asm -*-
# mach: all
# as: -isa=shmedia
# ld: -m shelf64

	.include "media/testutils.inc"

	start
init:
	pta wrong, tr0
	movi 9, r0
	fmov.ls r0, fr7
	float.ld fr7, dr0
	movi 3, r1
	fmov.ls r1, fr7
	float.ld fr7, dr2

fsqrtd:
	fsqrt.d dr0, dr4
	fcmpeq.d dr2, dr4, r7
	bnei r7, 1, tr0

okay:
	pass

wrong:
	fail
