# sh testcase for mov.b $rm, @$rn -*- Asm -*-
# mach: all
# as: -isa=shcompact
# ld: -m shelf32

	.include "compact/testutils.inc"

	start
	mov #55, r1
	mov #40, r2
	shll8 r2
	mov.b r1, @r2

	# Load it back into r3.
	mov #40, r2
	shll8 r2
	mov.b @r2, r3

	# Make sure r1 and r3 match.
	cmp/eq r1, r3
	bf wrong

okay:	
	pass

wrong:
	fail
