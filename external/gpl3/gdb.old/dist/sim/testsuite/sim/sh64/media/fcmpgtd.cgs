# sh testcase for fcmpgt.d $drg, $drh, $rd -*- Asm -*-
# mach: all
# as: -isa=shmedia
# ld: -m shelf64

	.include "media/testutils.inc"

	start

init:
	pta wrong, tr0

fcmpgt1:
	movi 2, r0
	fmov.qd r0, dr0
	movi 1, r1
	fmov.qd r1, dr2
	float.qd dr0, dr4
	float.qd dr2, dr6
	fcmpgt.d dr4, dr6, r7
	bnei r7, 1, tr0 

fcmpgt2:
	movi 1, r0
	fmov.qd r0, dr0
	fmov.qd r0, dr2
	float.qd dr0, dr4
	float.qd dr2, dr6
	fcmpgt.d dr4, dr6, r7
	bnei r7, 0, tr0

okay:
	pass

wrong:
	fail
