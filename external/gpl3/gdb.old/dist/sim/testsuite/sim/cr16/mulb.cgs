# cr16 testcase for mulb $imm4/imm16/reg,$reg
# mach(): cr16 

	.include "testutils.inc"

	start

	.global mulb
mulb:
	movw $0x1234,r4
	movw $0x4567,r5

	mulb r4, r5
	cmpb $0xec, r5
	beq ok1
not_ok:
	fail

ok1:
	movw $3,r4
	mulb $7,r4
	cmpb $21, r4
	beq ok
	br not_ok
ok:
	movw $3,r4
	mulb $0x1207, r4
	test_h_gr r4, 21

	pass
