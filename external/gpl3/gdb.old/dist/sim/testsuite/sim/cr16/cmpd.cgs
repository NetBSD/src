# cr16 testcase for cmpd (regp), (regp)
# mach(): cr16 

	.include "testutils.inc"

	start

	.global cmpd
cmpd:
	mvi_h_condbit 0
	movd $0x12345678, (r4,r3)
	movd $0x12345678, (r6,r5)
	cmpd (r4,r3), (r6,r5)
	beq ok
not_ok:
	fail
ok:
	mvi_h_condbit 1
	movd $0x12341234, (r6,r5)
	cmpd (r4,r3), (r6,r5)
	beq not_ok

	pass
