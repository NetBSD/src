# cr16 testcase for pop count reg insns.
# mach:	 cr16

	.include "testutils.inc"

	start

	.global pop2
pop2:
	movd $0x1000, (sp)
	movw  $0x2f50, r3
	storw r3, 0x1000
	movw  $0x107e, r3
	storw r3, 0x1002
	movw $0x35ec, r3
	storw r3, 0x1004

	pop $3,r5

	cmpw $0x2f50,r5
	beq ok1
	br not_ok
not_ok:
	fail
ok1:
	cmpw $0x107e,r6
	beq ok2
	br not_ok
ok2:
	cmpw $0x35ec,r7
	beq ok3
	br not_ok

ok3:
	pass
