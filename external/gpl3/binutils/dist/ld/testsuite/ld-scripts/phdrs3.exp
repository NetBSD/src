# PHDRS is only meaningful for ELF.
if ![is_elf_format] {
    return
}

load_lib ld-lib.exp

set testname "PHDRS headers"

set old_ldflags $LDFLAGS
if { [istarget spu*-*-*] } {
    set LDFLAGS "$LDFLAGS --no-overlays --local-store 0:0"
}

run_dump_test "phdrs3"
run_dump_test "phdrs3a"

set LDFLAGS $old_ldflags
