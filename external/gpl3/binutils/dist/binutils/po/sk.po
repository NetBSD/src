#: ar.c:233
#, c-format
msgid "  d            - delete file(s) from the archive\n"
msgstr "  d            - odstrániť súbor(y) z archívu\n"

#: ar.c:234
#, c-format
msgid "  m[ab]        - move file(s) in the archive\n"
