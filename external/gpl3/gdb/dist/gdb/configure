frags=
if test "${gdb_native}" = "yes"; then
  host_makefile_frag=${srcdir}/config/${gdb_host_cpu}/${gdb_host}.mh
  if test ! -f ${host_makefile_frag}; then
    as_fn_error "\"*** Gdb does not support native target ${host}\"" "$LINENO" 5
