              done
            fi
          else
                                                            LIBEXPAT="${LIBEXPAT}${LIBEXPAT:+ }-l$name"
            LTLIBEXPAT="${LTLIBEXPAT}${LTLIBEXPAT:+ }-l$name"
          fi
        fi
