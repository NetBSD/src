gdb_exit
gdb_start
gdb_reinitialize_dir $srcdir/$subdir

gdb_test_no_output "set lang ada" \
         "set lang ada"

gdb_test "ptype 3 * 2.0" \
         "= <\[0-9\]+-byte float>" \
         "ptype 3 * 2.0"

gdb_test "ptype 3 / 2.0" \
         "= <\[0-9\]+-byte float>" \
         "ptype 3 / 2.0"


