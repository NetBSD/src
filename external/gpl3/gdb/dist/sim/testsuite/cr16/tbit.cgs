# cr16 testcase for tbit
# mach:	 cr16

	.include "testutils.inc"

	start

	.global tbit
tbit:
	movw $0, r1
	lpr  r1, psr
	movw $0x7, r1
	tbit $0, r1
	spr  psr, r1
	cmpb $0x20, r1
	beq ok1
not_ok:
	fail

ok1:
	movw $0, r1
	lpr     r1, psr
	movw $0xa, r1
	movw $0x1, r2
	tbit r2,r1
	spr  psr, r1
	cmpb $0x20, r1
	beq ok2
	br not_ok
ok2:
	pass
