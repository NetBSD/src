# cr16 testcase for push count reg insns.
# mach:	 cr16

	.include "testutils.inc"

	start

	.global push2
push2:
	movd $0x1006, (sp)
	movw $0x2f50, r5
	movw $0x107e, r6
	movw $0x35ed, r7
	push $3,r5

	loadw 0x1000, r3
	cmpw r3,r5
	beq ok1
	br not_ok
not_ok:
	fail
ok1:
	loadw 0x1002, r3
	cmpw r3,r6
	beq ok2
	br not_ok
ok2:
	loadw 0x1004, r3
	cmpw r3,r7
	beq ok3
	br not_ok

ok3:
	pass

	
