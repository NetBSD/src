#!/bin/sh
# ad hoc debug tool

x=$1
y=$2

xout=`basename $x`.xxx.$$
yout=`basename $x`.yyy.$$

mkdir $xout
mkdir $yout

for i in *.s
do
	echo Testing $i...
	object=`basename $i .s`.o
	$x $i -o $xout/$object
	$y $i -o $yout/$object

# if they cmp, we're ok.  Otherwise we have to look closer.

	if (cmp $xout/$object $yout/$object)
	then
		echo $i is ok.
	else
		if (doobjcmp $xout/$object $yout/$object)
		then
			echo Not the same but objcmp ok.
		else
			exit 1
		fi
	fi

	echo
done

rm -rf $xout $yout

exit 0

# EOF


