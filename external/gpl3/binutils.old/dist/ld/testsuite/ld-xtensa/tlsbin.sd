#source: tlsbin.s
#as:
#ld: -melf32xtensa
#objdump: -sj.text --stop-address=0x400238
#target: xtensa*-*-linux*

.*: +file format elf32-xtensa-.e

Contents of section .text:
 *[0-9a-f]+ 0+ 0+ 0+ 0+  .*
 *[0-9a-f]+ 0+ 0*080* 0+ 0*280*  .*
 *[0-9a-f]+ 0+ 0*480* 0+ 0*080*  .*
 *[0-9a-f]+ 0*290* 0*2e0* 0*480* 0*4f0*  .*
 *[0-9a-f]+ 0+ 0*080* 0*280* 0*480*  .*
