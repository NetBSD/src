#! /bin/sh
# Set proper variables to allow easy "make" building of a NetBSD subtree.
# Generated from:  $NetBSD: build.sh,v 1.372 2023/06/13 16:56:00 christos Exp $
# with these arguments: -U -O build/obj -j4 -m soft68k -a m68k tools
#

BSHELL=/bin/sh; export BSHELL
BUILDSEED=NetBSD-11; export BUILDSEED
CONFIG_SHELL=/bin/sh; export CONFIG_SHELL
unset CPLUS_INCLUDE_PATH
unset C_INCLUDE_PATH
DESTDIR=/Users/sidqian/Downloads/summer/L2S/netbsd-src/build/obj/destdir.soft68k; export DESTDIR
HOST_SH=/bin/sh; export HOST_SH
unset INFODIR
LC_ALL=C; export LC_ALL
unset LESSCHARSET
MACHINE=soft68k; export MACHINE
MACHINE_ARCH=m68k; export MACHINE_ARCH
MAKEFLAGS='-de -m /Users/sidqian/Downloads/summer/L2S/netbsd-src/share/mk -X  MKOBJDIRS=yes'; export MAKEFLAGS
MAKEOBJDIR='${.CURDIR:C,^/Users/sidqian/Downloads/summer/L2S/netbsd-src,/Users/sidqian/Downloads/summer/L2S/netbsd-src/build/obj,}'; export MAKEOBJDIR
unset MAKEOBJDIRPREFIX
MAKEWRAPPERMACHINE=soft68k; export MAKEWRAPPERMACHINE
MAKE_OBJDIR_CHECK_WRITABLE=no; export MAKE_OBJDIR_CHECK_WRITABLE
MKARZERO=yes; export MKARZERO
MKUNPRIVED=yes; export MKUNPRIVED
NETBSDSRCDIR=/Users/sidqian/Downloads/summer/L2S/netbsd-src; export NETBSDSRCDIR
RELEASEDIR=/Users/sidqian/Downloads/summer/L2S/netbsd-src/build/obj/releasedir; export RELEASEDIR
unset TERMINFO
TOOLDIR='/Users/sidqian/Downloads/summer/L2S/netbsd-src/build/obj/tooldir.Darwin-22.1.0-x86_64'; export TOOLDIR
USETOOLS=yes; export USETOOLS

exec "${TOOLDIR}/bin/nbmake" ${1+"$@"}
