#!/bin/sh
#
# $NetBSD: pflogd,v 1.1 2004/06/29 04:48:28 itojun Exp $
#

# PROVIDE: pflogd
# REQUIRE: pf
# BEFORE:  SERVERS

. /etc/rc.subr

name="pflogd"
rcvar=$name
command="/sbin/${name}"
start_precmd="/sbin/ifconfig pflog0 up"
pidfile="/var/run/${name}.pid"

load_rc_config $name
run_rc_command "$1"
