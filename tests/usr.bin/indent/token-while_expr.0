/*
 * Tests for the keyword 'while', followed by a parenthesized expression.
 */

int main(int argc,char**argv){int o;while((o=getopt(argc,argv,"x:"))!=-1)
switch(o){case'x':do{o++;}while(o<5);break;default:usage();}return 0;}
