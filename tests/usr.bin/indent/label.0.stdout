/*
 * The indentation of statement labels is fixed to -2, it is not configurable.
 */
void
label_indentation(void)
{
        if (level1) {
                if (level2) {
                        if (level3) {
                                if (level4) {
                                        if (level5) {
                                label5:
                                                statement();
                                        }
                        label4:
                                        statement();
                                }
                label3:
                                statement();
                        }
        label2:
                        statement();
                }
label1:
                statement();
        }
label0:
        statement();
}
