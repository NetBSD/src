LIB=	rumpdev
COMMENT=Rump kernel device faction

.PATH:	${RUMPTOP}/librump/rumpdev				\
	${RUMPTOP}/../kern					\
	${RUMPTOP}/../dev

SRCS=	rump_dev.c

# sys/kern
