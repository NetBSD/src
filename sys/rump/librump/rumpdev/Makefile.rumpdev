COPTS.dev_verbose.c+= -Wno-error=format-nonliteral

CPPFLAGS+=	-I${RUMPTOP}/librump/rumpkern

.include "${RUMPTOP}/Makefile.rump"

.include <bsd.lib.mk>
.include <bsd.klinks.mk>
