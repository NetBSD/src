				reg = <0x15bc0000 0x40000>;
				interrupts = <GIC_SPI 244 IRQ_TYPE_LEVEL_HIGH>;
				clocks = <&bpmp TEGRA194_CLK_SOR3_REF>,
					 <&bpmp TEGRA194_CLK_SOR3_OUT>,
					 <&bpmp TEGRA194_CLK_PLLD4>,
