					interrupts = <GIC_SPI 153 IRQ_TYPE_LEVEL_HIGH>;
					clocks = <&bpmp TEGRA194_CLK_NVDISPLAY_P0>;
					clock-names = "dc";
					resets = <&bpmp TEGRA194_RESET_NVDISPLAY0_HEAD0>;
					reset-names = "dc";

					power-domains = <&bpmp TEGRA194_POWER_DOMAIN_DISP>;

