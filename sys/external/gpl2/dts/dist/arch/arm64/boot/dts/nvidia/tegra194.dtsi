				status = "disabled";

				power-domains = <&bpmp TEGRA194_POWER_DOMAIN_DISP>;

				state_dpaux0_aux: pinmux-aux {
					groups = "dpaux-io";
					function = "aux";
				};

				state_dpaux0_i2c: pinmux-i2c {
					groups = "dpaux-io";
					function = "i2c";
