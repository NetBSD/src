CPPFLAGS+=	-I${S}/dist/pf -I${S} -DINET6 -DINET

.if ${HAVE_GCC:U0} == "6"
COPTS.pf_table.c+=	-Wno-error=shift-negative-value
.endif

.include <bsd.kmodule.mk>
