#! /bin/sh -e
# $NetBSD: filelist,v 1.1.2.1 2007/05/22 14:57:35 itohy Exp $
#	List of files in branch
#
: ${base='-D2007-03-01'}
: ${branch=itohy-usb1} ${cvsprog=cvs}
cd `dirname "$0"`	# src/sys/dev/usb
cd ../..		# src/sys
${cvsprog} update $base -dP
sed -e '1,/^### FILE LIST ###/d' -e '/^#/d' <<'###END' | xargs ${cvsprog} update -r${branch}; exit

### FILE LIST ###
# branch-only files
#dev/usb/filelist
#dev/usb/api.txt
#dev/usb/usb_mem_nodma.c
#dev/usb/usb_mem_nodma.h
#dev/pcmcia/slhci_pcmcia.c

dev/usb
arch/x68k/dev/slhci_intio.c
conf/files
dev/cardbus/ehci_cardbus.c
dev/cardbus/ohci_cardbus.c
dev/cardbus/uhci_cardbus.c
dev/ic/sl811hs.c
dev/ic/sl811hsvar.h
dev/isa/slhci_isa.c
dev/pci/ehci_pci.c
dev/pci/ohci_pci.c
dev/pci/uhci_pci.c
dev/pcmcia/files.pcmcia
dev/pcmcia/slhci_pcmcia.c
