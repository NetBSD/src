# The main bus device
mainbus0 at root

# The boot CPU
cpu0 at mainbus?

saip0	at mainbus?

# Serial
options 	CONSPEED=19200
sacom*	at saip? addr 0x80050000 size 0x24	intr 17

# OS Timer
saost*	at saip? addr 0x90000000 size 0x20

# iPAQ bus
