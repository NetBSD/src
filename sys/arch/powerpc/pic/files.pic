# generic PIC abstraction

file arch/powerpc/pic/intr.c
defflag opt_pic.h PIC_DEBUG

define pic_openpic
define pic_distopenpic
define pic_prepivr
define pic_i8259
define pic_mpcsoc

defflag opt_interrupt.h PIC_OPENPIC:	pic_openpic
