makeoptions	LP64="yes"

no options 	MIPS32
options 	EXEC_ELF64
options 	COMPAT_NETBSD32
no makeoptions	COPY_SYMTAB
makeoptions	COPY_SYMTAB=1	# size for embedded symbol table

#options 	UVMHIST, UVMHIST_PRINT

no ath*
