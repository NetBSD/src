include "arch/evbmips/conf/MALTA"

makeoptions	LP64="yes"

no options 	MIPS32
options 	EXEC_ELF64
options 	COMPAT_NETBSD32
no options 	SYMTAB_SPACE
options 	SYMTAB_SPACE=490000	# size for embedded symbol table

#options 	UVMHIST, UVMHIST_PRINT

no ath*
