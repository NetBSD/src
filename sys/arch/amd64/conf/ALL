options 	VERIFIED_EXEC_FP_SHA256
options 	VERIFIED_EXEC_FP_SHA384
options 	VERIFIED_EXEC_FP_SHA512
options 	VERIFIED_EXEC_FP_SHA1
options 	VERIFIED_EXEC_FP_MD5

