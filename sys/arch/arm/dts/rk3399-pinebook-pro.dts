	usb2 {
		vcc5v0_host_en: vcc5v0-host-en {
			rockchip,pins = <4 RK_PD2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	i2s1 {
		i2s_8ch_mclk: i2s-8ch-mclk {
			rockchip,pins = <4 0 RK_FUNC_1 &pcfg_pull_none>;
