# $NetBSD: INSTALL_C700,v 1.1.28.1 2016/10/05 20:55:37 skrll Exp $
#
# INSTALL config file (C700 with memory disk root)
#
# Note: C700 seems to have more size restriction.
#

include 	"arch/zaurus/conf/INSTALL"

no options 	CPU_XSCALE_PXA270

no makeoptions	KERNEL_BASE_PHYS
makeoptions	KERNEL_BASE_PHYS="0xa0400000"
no makeoptions	LOADADDRESS
makeoptions	LOADADDRESS="0xc0400000"

no scoop1 at pxaip?
no ioexp0 at iic?

no lcd0 at pxaip?
no wsdisplay* at lcd?

no zusb*
no ohci0
no usb*
no uhub* 

no uhidev*
no ukbd*
no wskbd* at ukbd?
no uhid*

no umass*
no wd* at umass?

no scsibus*
no atapibus*
no sd*
no cd*

no aue*
no axe*
no cdce*
no cue*
no kue*
no udav*
no url*
