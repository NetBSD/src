#	$NetBSD: PCCONS,v 1.51.6.1 2006/04/22 11:37:16 simonb Exp $
#
#	PCCONS -- generic but pccons rather than wscons
#

include		"arch/arc/conf/GENERIC"

no options 	WSEMUL_VT100	# VT100 emulation

pc0 at jazzio?
opms0 at jazzio?

no vga0 at jazzio?
no pckbc0 at jazzio?

pc0 at isa? irq 1
opms0 at isa? irq 12

no vga0 at isa?
#no options 	VGA_RASTERCONSOLE
no options 	VGA_RESET
no pckbc0 at isa?

#no vga* at pci?
no tga* at pci?

no wsdisplay* at tga?
no wsdisplay* at vga?
no pckbd* at pckbc?
no wskbd* at pckbd?
no pms* at pckbc?
no wsmouse* at pms?
