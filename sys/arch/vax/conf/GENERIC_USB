# $NetBSD: GENERIC_USB,v 1.1.12.1 2019/06/10 22:06:51 christos Exp $
#
# GENERIC kernel with USB

include	"arch/vax/conf/GENERIC"

# TC-USB
tcu*	at tc? slot ? offset ?
slhci*	at tcu?
gpio*	at gpiobus?

# USB bus support
usb*	at slhci?

include	"dev/usb/usbdevices.config"

midi*	at midibus?

rgephy*	at mii? phy ?
rlphy*	at mii? phy ?
ukphy*	at mii? phy ?
