include "arch/sandpoint/conf/GENERIC"

no com0
no satmgr0
com0	at eumb? unit 1 flags 0x1	# console at 0x4600
					# XXX flags 0x1 to work around H/W bug
#com1	at eumb? unit 0
satmgr0 at eumb? unit 0			# satmgr at 0x4500

no nhpow0
no gpio*

no s390rtc*
no pcf8563rtc*
no dsrtc*
no strtc*

no acardide*
no viaide* 

no ral*
no skc*
no sk*
no stge*
no wm*

no inphy*
no nsphy*
no makphy*
