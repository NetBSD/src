# $NetBSD: GENERIC64_USB,v 1.1.22.1 2019/06/10 22:06:37 christos Exp $
#
# GENERIC64 kernel with USB

include	"arch/pmax/conf/GENERIC64"

# TC-USB
tcu*	at tc? slot ? offset ?
slhci*	at tcu?
gpio*	at gpiobus?

# USB bus support
usb*	at slhci?

include	"dev/usb/usbdevices.config"

midi*	at midibus?

rgephy*	at mii? phy ?
rlphy*	at mii? phy ?
ukphy*	at mii? phy ?
